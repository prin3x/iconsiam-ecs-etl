generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model api_sync_logs {
  id                                              Int                                               @id @default(autoincrement())
  sync_id                                         String                                            @unique(map: "api_sync_logs_sync_id_idx") @db.VarChar
  status                                          enum_api_sync_logs_status                         @default(RUNNING)
  started_at                                      DateTime                                          @db.Timestamptz(3)
  completed_at                                    DateTime?                                         @db.Timestamptz(3)
  duration_seconds                                Decimal?                                          @db.Decimal
  external_api_url                                String                                            @db.VarChar
  api_response_summary_total_records_fetched      Decimal?                                          @default(0) @db.Decimal
  api_response_summary_total_pages                Decimal?                                          @default(0) @db.Decimal
  api_response_summary_last_page_processed        Decimal?                                          @default(0) @db.Decimal
  processing_summary_records_processed            Decimal?                                          @default(0) @db.Decimal
  processing_summary_records_created              Decimal?                                          @default(0) @db.Decimal
  processing_summary_records_updated              Decimal?                                          @default(0) @db.Decimal
  processing_summary_records_failed               Decimal?                                          @default(0) @db.Decimal
  processing_summary_shops_processed              Decimal?                                          @default(0) @db.Decimal
  processing_summary_dinings_processed            Decimal?                                          @default(0) @db.Decimal
  performance_metrics_avg_time_per_record         Decimal?                                          @db.Decimal
  performance_metrics_memory_usage_mb             Decimal?                                          @db.Decimal
  performance_metrics_api_response_time_avg       Decimal?                                          @db.Decimal
  notes                                           String?                                           @db.VarChar
  updated_at                                      DateTime                                          @default(now()) @db.Timestamptz(3)
  created_at                                      DateTime                                          @default(now()) @db.Timestamptz(3)
  api_sync_logs_errors                            api_sync_logs_errors[]
  api_sync_logs_unmapped_data_unmapped_categories api_sync_logs_unmapped_data_unmapped_categories[]
  api_sync_logs_unmapped_data_unmapped_floors     api_sync_logs_unmapped_data_unmapped_floors[]
  api_sync_logs_validation_issues                 api_sync_logs_validation_issues[]
  payload_locked_documents_rels                   payload_locked_documents_rels[]

  @@index([created_at])
  @@index([updated_at])
}

model api_sync_logs_errors {
  order            Int           @map("_order")
  parent_id        Int           @map("_parent_id")
  id               String        @id @db.VarChar
  timestamp        DateTime?     @db.Timestamptz(3)
  record_unique_id String?       @db.VarChar
  error_message    String?       @db.VarChar
  error_stack      String?       @db.VarChar
  api_sync_logs    api_sync_logs @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "api_sync_logs_errors_parent_id_fk")

  @@index([order], map: "api_sync_logs_errors_order_idx")
  @@index([parent_id], map: "api_sync_logs_errors_parent_id_idx")
}

model api_sync_logs_unmapped_data_unmapped_categories {
  order         Int                                                        @map("_order")
  parent_id     Int                                                        @map("_parent_id")
  id            String                                                     @id @db.VarChar
  category_name String?                                                    @db.VarChar
  type          enum_api_sync_logs_unmapped_data_unmapped_categories_type?
  english_name  String?                                                    @db.VarChar
  thai_name     String?                                                    @db.VarChar
  api_sync_logs api_sync_logs                                              @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "api_sync_logs_unmapped_data_unmapped_categories_parent_id_fk")

  @@index([order], map: "api_sync_logs_unmapped_data_unmapped_categories_order_idx")
  @@index([parent_id], map: "api_sync_logs_unmapped_data_unmapped_categories_parent_id_idx")
}

model api_sync_logs_unmapped_data_unmapped_floors {
  order         Int           @map("_order")
  parent_id     Int           @map("_parent_id")
  id            String        @id @db.VarChar
  floor_name    String?       @db.VarChar
  api_sync_logs api_sync_logs @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "api_sync_logs_unmapped_data_unmapped_floors_parent_id_fk")

  @@index([order], map: "api_sync_logs_unmapped_data_unmapped_floors_order_idx")
  @@index([parent_id], map: "api_sync_logs_unmapped_data_unmapped_floors_parent_id_idx")
}

model api_sync_logs_validation_issues {
  order            Int           @map("_order")
  parent_id        Int           @map("_parent_id")
  id               String        @id @db.VarChar
  record_unique_id String?       @db.VarChar
  description      String?       @db.VarChar
  api_sync_logs    api_sync_logs @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "api_sync_logs_validation_issues_parent_id_fk")

  @@index([order], map: "api_sync_logs_validation_issues_order_idx")
  @@index([parent_id], map: "api_sync_logs_validation_issues_parent_id_idx")
}

model attractions {
  id                                                 Int                               @id @default(autoincrement())
  main_image_url_id                                  Int?
  feature_image_url_id                               Int?
  showcase_image_url_id                              Int?
  gallery_main_image_url_id                          Int?
  status                                             enum_attractions_status           @default(ACTIVE)
  is_featured                                        Boolean?
  updated_at                                         DateTime                          @default(now()) @db.Timestamptz(3)
  created_at                                         DateTime                          @default(now()) @db.Timestamptz(3)
  slug                                               String?                           @db.VarChar
  order                                              Decimal                           @db.Decimal
  slug_lock                                          Boolean?                          @default(true)
  showcase_background_color                          String?                           @default("#000000") @db.VarChar
  media_attractions_feature_image_url_idTomedia      media?                            @relation("attractions_feature_image_url_idTomedia", fields: [feature_image_url_id], references: [id], onUpdate: NoAction, map: "attractions_feature_image_url_id_media_id_fk")
  media_attractions_gallery_main_image_url_idTomedia media?                            @relation("attractions_gallery_main_image_url_idTomedia", fields: [gallery_main_image_url_id], references: [id], onUpdate: NoAction, map: "attractions_gallery_main_image_url_id_media_id_fk")
  media_attractions_main_image_url_idTomedia         media?                            @relation("attractions_main_image_url_idTomedia", fields: [main_image_url_id], references: [id], onUpdate: NoAction, map: "attractions_main_image_url_id_media_id_fk")
  media_attractions_showcase_image_url_idTomedia     media?                            @relation("attractions_showcase_image_url_idTomedia", fields: [showcase_image_url_id], references: [id], onUpdate: NoAction, map: "attractions_showcase_image_url_id_media_id_fk")
  attractions_gallery_image_urls                     attractions_gallery_image_urls[]
  attractions_highlight_image_url                    attractions_highlight_image_url[]
  attractions_keywords                               attractions_keywords[]
  attractions_locales                                attractions_locales[]
  directory_rels                                     directory_rels[]
  events_rels                                        events_rels[]
  homepage_rels                                      homepage_rels[]
  news_press_rels                                    news_press_rels[]
  payload_locked_documents_rels                      payload_locked_documents_rels[]
  promotions_rels                                    promotions_rels[]
  stories_rels                                       stories_rels[]

  @@index([created_at])
  @@index([feature_image_url_id], map: "attractions_feature_image_url_idx")
  @@index([gallery_main_image_url_id], map: "attractions_gallery_main_image_url_idx")
  @@index([main_image_url_id], map: "attractions_main_image_url_idx")
  @@index([showcase_image_url_id], map: "attractions_showcase_image_url_idx")
  @@index([slug])
  @@index([updated_at])
}

model attractions_gallery_image_urls {
  order        Int         @map("_order")
  parent_id    Int         @map("_parent_id")
  id           String      @id @db.VarChar
  image_url_id Int?
  media        media?      @relation(fields: [image_url_id], references: [id], onUpdate: NoAction, map: "attractions_gallery_image_urls_image_url_id_media_id_fk")
  attractions  attractions @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "attractions_gallery_image_urls_parent_id_fk")

  @@index([image_url_id], map: "attractions_gallery_image_urls_image_url_idx")
  @@index([order], map: "attractions_gallery_image_urls_order_idx")
  @@index([parent_id], map: "attractions_gallery_image_urls_parent_id_idx")
}

model attractions_highlight_image_url {
  order        Int         @map("_order")
  parent_id    Int         @map("_parent_id")
  id           String      @id @db.VarChar
  image_url_id Int?
  media        media?      @relation(fields: [image_url_id], references: [id], onUpdate: NoAction, map: "attractions_highlight_image_url_image_url_id_media_id_fk")
  attractions  attractions @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "attractions_highlight_image_url_parent_id_fk")

  @@index([image_url_id], map: "attractions_highlight_image_url_image_url_idx")
  @@index([order], map: "attractions_highlight_image_url_order_idx")
  @@index([parent_id], map: "attractions_highlight_image_url_parent_id_idx")
}

model attractions_keywords {
  order       Int         @map("_order")
  parent_id   Int         @map("_parent_id")
  locale      locales     @map("_locale")
  id          String      @id @db.VarChar
  keyword     String?     @db.VarChar
  attractions attractions @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "attractions_keywords_parent_id_fk")

  @@index([locale], map: "attractions_keywords_locale_idx")
  @@index([order], map: "attractions_keywords_order_idx")
  @@index([parent_id], map: "attractions_keywords_parent_id_idx")
}

model attractions_locales {
  title                String?     @db.VarChar
  subtitle             String?     @db.VarChar
  description          String?     @db.VarChar
  feature_title        String?     @db.VarChar
  feature_description  String?     @db.VarChar
  showcase_title       String?     @db.VarChar
  showcase_description String?     @db.VarChar
  gallery_title        String?     @db.VarChar
  gallery_description  String?     @db.VarChar
  meta_title           String?     @db.VarChar
  meta_description     String?     @db.VarChar
  id                   Int         @id @default(autoincrement())
  locale               locales     @map("_locale")
  parent_id            Int         @map("_parent_id")
  showcase_subtitle    String?     @db.VarChar
  attractions          attractions @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "attractions_locales_parent_id_fk")

  @@unique([locale, parent_id], map: "attractions_locales_locale_parent_id_unique")
}

model categories {
  id                            Int                             @id @default(autoincrement())
  image_id                      Int?
  type                          enum_categories_type
  order                         Decimal?                        @default(0) @db.Decimal
  status                        enum_categories_status          @default(ACTIVE)
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  slug                          String                          @unique(map: "categories_slug_idx") @db.VarChar
  original_id                   String?                         @db.VarChar
  pin_to_directory              Boolean?                        @default(false)
  media                         media?                          @relation(fields: [image_id], references: [id], onUpdate: NoAction, map: "categories_image_id_media_id_fk")
  categories_locales            categories_locales[]
  dinings_rels                  dinings_rels[]
  events_rels                   events_rels[]
  news_press_rels               news_press_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]
  promotions_rels               promotions_rels[]
  shops_rels                    shops_rels[]
  stories_rels                  stories_rels[]

  @@index([created_at])
  @@index([image_id], map: "categories_image_idx")
  @@index([original_id])
  @@index([updated_at])
}

model categories_locales {
  name         String?    @db.VarChar
  description  String?    @db.VarChar
  id           Int        @id @default(autoincrement())
  locale       locales    @map("_locale")
  parent_id    Int        @map("_parent_id")
  display_name String?    @db.VarChar
  categories   categories @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "categories_locales_parent_id_fk")

  @@unique([locale, parent_id], map: "categories_locales_locale_parent_id_unique")
}

model dinings {
  id                                            Int                             @id @default(autoincrement())
  floor_id                                      Int?
  location_zone                                 String?                         @db.VarChar
  opening_hours_same_hours_every_day            Boolean?                        @default(true)
  opening_hours_open                            String?                         @db.VarChar
  opening_hours_close                           String?                         @db.VarChar
  status                                        enum_dinings_status             @default(ACTIVE)
  is_featured                                   Boolean?
  updated_at                                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                                    DateTime                        @default(now()) @db.Timestamptz(3)
  slug                                          String?                         @db.VarChar
  slug_lock                                     Boolean?                        @default(true)
  location_coordinates_poi_x                    Decimal?                        @default(0) @db.Decimal
  location_coordinates_poi_y                    Decimal?                        @default(0) @db.Decimal
  contact_info_website                          String?                         @db.VarChar
  contact_info_phone                            String?                         @db.VarChar
  contact_info_instagram_url                    String?                         @db.VarChar
  contact_info_facebook_url                     String?                         @db.VarChar
  contact_info_wechat_account                   String?                         @db.VarChar
  contact_info_line_account                     String?                         @db.VarChar
  contact_info_weibo_url                        String?                         @db.VarChar
  images_logo_id                                Int?
  images_main_image_id                          Int?
  images_thumbnail_id                           Int?
  images_facebook_image_id                      Int?
  date_range_start_date                         DateTime?                       @db.Timestamptz(3)
  date_range_end_date                           DateTime?                       @db.Timestamptz(3)
  short_alphabet                                String?                         @db.VarChar
  sort_order                                    Decimal?                        @db.Decimal
  unique_id                                     String?                         @db.VarChar
  pin_to_iconluxe                               Boolean?                        @default(false)
  floors                                        floors?                         @relation(fields: [floor_id], references: [id], onUpdate: NoAction, map: "dinings_floor_id_floors_id_fk")
  media_dinings_images_facebook_image_idTomedia media?                          @relation("dinings_images_facebook_image_idTomedia", fields: [images_facebook_image_id], references: [id], onUpdate: NoAction, map: "dinings_images_facebook_image_id_media_id_fk")
  media_dinings_images_logo_idTomedia           media?                          @relation("dinings_images_logo_idTomedia", fields: [images_logo_id], references: [id], onUpdate: NoAction, map: "dinings_images_logo_id_media_id_fk")
  media_dinings_images_main_image_idTomedia     media?                          @relation("dinings_images_main_image_idTomedia", fields: [images_main_image_id], references: [id], onUpdate: NoAction, map: "dinings_images_main_image_id_media_id_fk")
  media_dinings_images_thumbnail_idTomedia      media?                          @relation("dinings_images_thumbnail_idTomedia", fields: [images_thumbnail_id], references: [id], onUpdate: NoAction, map: "dinings_images_thumbnail_id_media_id_fk")
  dinings_images_gallery                        dinings_images_gallery[]
  dinings_keywords                              dinings_keywords[]
  dinings_locales                               dinings_locales[]
  dinings_opening_hours_per_day                 dinings_opening_hours_per_day[]
  dinings_rels_dinings_rels_dinings_idTodinings dinings_rels[]                  @relation("dinings_rels_dinings_idTodinings")
  dinings_rels_dinings_rels_parent_idTodinings  dinings_rels[]                  @relation("dinings_rels_parent_idTodinings")
  dinings_tags                                  dinings_tags[]
  directory_rels                                directory_rels[]
  events_rels                                   events_rels[]
  homepage_dinings                              homepage_dinings[]
  homepage_rels                                 homepage_rels[]
  news_press_rels                               news_press_rels[]
  payload_locked_documents_rels                 payload_locked_documents_rels[]
  promotions_rels                               promotions_rels[]
  shops_rels                                    shops_rels[]
  stories_rels                                  stories_rels[]

  @@index([created_at])
  @@index([floor_id], map: "dinings_floor_idx")
  @@index([images_facebook_image_id], map: "dinings_images_images_facebook_image_idx")
  @@index([images_logo_id], map: "dinings_images_images_logo_idx")
  @@index([images_main_image_id], map: "dinings_images_images_main_image_idx")
  @@index([images_thumbnail_id], map: "dinings_images_images_thumbnail_idx")
  @@index([slug])
  @@index([updated_at])
}

model dinings_images_gallery {
  order     Int     @map("_order")
  parent_id Int     @map("_parent_id")
  id        String  @id @db.VarChar
  image_id  Int?
  media     media?  @relation(fields: [image_id], references: [id], onUpdate: NoAction, map: "dinings_images_gallery_image_id_media_id_fk")
  dinings   dinings @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_images_gallery_parent_id_fk")

  @@index([image_id], map: "dinings_images_gallery_image_idx")
  @@index([order], map: "dinings_images_gallery_order_idx")
  @@index([parent_id], map: "dinings_images_gallery_parent_id_idx")
}

model dinings_keywords {
  order     Int     @map("_order")
  parent_id Int     @map("_parent_id")
  locale    locales @map("_locale")
  id        String  @id @db.VarChar
  keyword   String? @db.VarChar
  dinings   dinings @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_keywords_parent_id_fk")

  @@index([locale], map: "dinings_keywords_locale_idx")
  @@index([order], map: "dinings_keywords_order_idx")
  @@index([parent_id], map: "dinings_keywords_parent_id_idx")
}

model dinings_locales {
  title                String? @db.VarChar
  subtitle             String? @db.VarChar
  description          String? @db.VarChar
  location_shop_number String? @db.VarChar
  meta_title           String? @db.VarChar
  meta_description     String? @db.VarChar
  id                   Int     @id @default(autoincrement())
  locale               locales @map("_locale")
  parent_id            Int     @map("_parent_id")
  dinings              dinings @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_locales_parent_id_fk")

  @@unique([locale, parent_id], map: "dinings_locales_locale_parent_id_unique")
}

model dinings_opening_hours_per_day {
  order     Int                                     @map("_order")
  parent_id Int                                     @map("_parent_id")
  id        String                                  @id @db.VarChar
  day       enum_dinings_opening_hours_per_day_day?
  open      String?                                 @db.VarChar
  close     String?                                 @db.VarChar
  dinings   dinings                                 @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_opening_hours_per_day_parent_id_fk")

  @@index([order], map: "dinings_opening_hours_per_day_order_idx")
  @@index([parent_id], map: "dinings_opening_hours_per_day_parent_id_idx")
}

model dinings_rels {
  id                                       Int         @id @default(autoincrement())
  order                                    Int?
  parent_id                                Int
  path                                     String      @db.VarChar
  categories_id                            Int?
  dinings_id                               Int?
  shops_id                                 Int?
  promotions_id                            Int?
  categories                               categories? @relation(fields: [categories_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_rels_categories_fk")
  dinings_dinings_rels_dinings_idTodinings dinings?    @relation("dinings_rels_dinings_idTodinings", fields: [dinings_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_rels_dinings_fk")
  dinings_dinings_rels_parent_idTodinings  dinings     @relation("dinings_rels_parent_idTodinings", fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_rels_parent_fk")
  promotions                               promotions? @relation(fields: [promotions_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_rels_promotions_fk")
  shops                                    shops?      @relation(fields: [shops_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_rels_shops_fk")

  @@index([categories_id])
  @@index([dinings_id])
  @@index([order])
  @@index([parent_id], map: "dinings_rels_parent_idx")
  @@index([path])
  @@index([promotions_id])
  @@index([shops_id])
}

model dinings_tags {
  order     Int     @map("_order")
  parent_id Int     @map("_parent_id")
  id        String  @id @db.VarChar
  tag       String? @db.VarChar
  dinings   dinings @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "dinings_tags_parent_id_fk")

  @@index([order], map: "dinings_tags_order_idx")
  @@index([parent_id], map: "dinings_tags_parent_id_idx")
}

model directory {
  id                            Int                             @id @default(autoincrement())
  updated_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  created_at                    DateTime                        @default(now()) @db.Timestamptz(3)
  directory_icon_siam_picks     directory_icon_siam_picks[]
  directory_rels                directory_rels[]
  payload_locked_documents_rels payload_locked_documents_rels[]

  @@index([created_at])
  @@index([updated_at])
}

model directory_icon_siam_picks {
  order                             Int                                 @map("_order")
  parent_id                         Int                                 @map("_parent_id")
  id                                String                              @id @db.VarChar
  custom_image_id                   Int?
  media                             media?                              @relation(fields: [custom_image_id], references: [id], onUpdate: NoAction, map: "directory_icon_siam_picks_custom_image_id_media_id_fk")
  directory                         directory                           @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "directory_icon_siam_picks_parent_id_fk")
  directory_icon_siam_picks_locales directory_icon_siam_picks_locales[]

  @@index([custom_image_id], map: "directory_icon_siam_picks_custom_image_idx")
  @@index([order], map: "directory_icon_siam_picks_order_idx")
  @@index([parent_id], map: "directory_icon_siam_picks_parent_id_idx")
}

model directory_icon_siam_picks_locales {
  custom_title              String?                   @db.VarChar
  id                        Int                       @id @default(autoincrement())
  locale                    locales                   @map("_locale")
  parent_id                 String                    @map("_parent_id") @db.VarChar
  directory_icon_siam_picks directory_icon_siam_picks @relation(fields: [parent_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "directory_icon_siam_picks_locales_parent_id_fk")

  @@unique([locale, parent_id], map: "directory_icon_siam_picks_locales_locale_parent_id_unique")
}

model directory_rels {
  id             Int          @id @default(autoincrement())
  order          Int?
  parent_id      Int
  path           String       @db.VarChar
  shops_id       Int?
  dinings_id     Int?
  attractions_id Int?
  icon_luxe_id   Int?
  icon_craft_id  Int?
  attractions    attractions? @relation(fields: [attractions_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "directory_rels_attractions_fk")
  dinings        dinings?     @relation(fields: [dinings_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "directory_rels_dinings_fk")
  icon_craft     icon_craft?  @relation(fields: [icon_craft_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "directory_rels_icon_craft_fk")
