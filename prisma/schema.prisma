  events_id      Int?
  promotions_id  Int?
  attractions    attractions? @relation(fields: [attractions_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "stories_rels_attractions_fk")
  categories     categories?  @relation(fields: [categories_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "stories_rels_categories_fk")
  dinings        dinings?     @relation(fields: [dinings_id], references: [id], onDelete: Cascade, onUpdate: NoAction, map: "stories_rels_dinings_fk")
