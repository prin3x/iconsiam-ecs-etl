  @@index([categories_id])
  @@index([dinings_id])
  @@index([events_id])
  @@index([order])
  @@index([parent_id], map: "stories_rels_parent_idx")
  @@index([path])
  @@index([promotions_id])
  @@index([shops_id])
}

